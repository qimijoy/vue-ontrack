{
  "extends": "@vue/tsconfig/tsconfig.dom.json",
  "include": ["env.d.ts", "config/**/*", "src/**/*", "src/**/*.vue"],
  "exclude": [],
  "compilerOptions": {
    "composite": true,
    "tsBuildInfoFile": "./node_modules/.tmp/tsconfig.app.tsbuildinfo",
    "allowJs": true,
    "checkJs": true,
    "esModuleInterop": true,
    "moduleResolution": "node",
    "skipLibCheck": true,
    "strict": false,

    "baseUrl": ".",
    "paths": {
      "@/*": ["./src/*"],
      "@config/*": ["./config/*"],
    },
    "types": ["node"]
  }
}
